{
  "name": "digiaalto-gatsby-boilerplate",
  "private": true,
  "description": "A modified Gatsby hello-world-starter by Digiaalto",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.24.50",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.27",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-sitemap": "^2.4.12",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-sharp": "^2.5.14",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "gatsby-disable-404": "^0.2.1",
    "prettier": "2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digiaalto/gatsby-boilerplate.git"
  }
}
